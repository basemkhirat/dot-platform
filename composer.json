{

    "name": "dot/platform",
    "description": "The core platform of dot/cms",
    "keywords": ["dotcms", "framework", "laravel", "cms", "modules", "plugins"],
    "license": "MIT",

    "type": "package",

    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4",
        "barryvdh/laravel-debugbar": "2.3.*",
        "roumen/sitemap": "2.*",
        "intervention/image": "2.*",
        "aws/aws-sdk-php-laravel": "2.0",
        "laravelcollective/html": "5.4.*",
        "phpdocumentor/reflection-docblock": "2.0@dev"
    },

    "autoload": {
        "classmap": [
            "src/bootstrap"
        ]
    },

    "config": {
        "preferred-install": "dist"
    },

    "minimum-stability" : "dev"
}
