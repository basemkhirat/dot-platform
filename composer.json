{
    "name": "dot/platform",
    "description": "Just a laravel cms",
    "keywords": ["framework", "laravel", "cms", "plugins"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.5",
        "barryvdh/laravel-debugbar": "2.*",
        "doctrine/dbal": "^2.5",
        "dot/auth": "dev-master",
        "dot/roles": "dev-master",
        "dot/users": "dev-master",
        "dot/options": "dev-master",
        "dot/media": "dev-master",
        "dot/galleries": "dev-master",
        "dot/dashboard": "dev-master"
    },
    "autoload": {
        "psr-4" : {
            "Dot\\Platform\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan dot:install"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability" : "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Dot\\Platform\\ServiceProvider"
            ]
        }
    }
}
